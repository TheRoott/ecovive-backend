{
  "version": 2,
  "buildCommand": "cd backend && mvn clean package -DskipTests",
  "outputDirectory": "backend/target",
  "installCommand": "cd backend && mvn clean package -DskipTests",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "vercel"
  }
}
