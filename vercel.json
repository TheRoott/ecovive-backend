{
  "version": 2,
  "builds": [
    {
      "src": "pom.xml",
      "use": "@vercel/java"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "vercel"
  },
  "functions": {
    "pom.xml": {
      "runtime": "java17"
    }
  }
}
